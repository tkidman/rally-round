{
  "name": "dirt2-results",
  "version": "1.0.0",
  "description": "dirt2 results",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "test-e2e": "jest e2e.test.js",
    "test:update-snapshots": "jest -u",
    "lint": "eslint src/",
    "validate-configs": "node src/state/editor/validate.js",
    "config-editor": "cd src/state/editor && node server.js",
    "prepush": "npm run lint -- --fix && npm run test && npm run validate-configs",
    "set-redirect": "node setRedirectFromConfig.js",
    "prepare-championship": "node prepareNewChampionship.js",
    "prepare-championship:parent": "node prepareChampionshipFromParent.js"
  },
  "repository": {
    "type": "git",
    "url": "tba"
  },
  "keywords": [
    "dirt2",
    "dirt",
    "rally",
    "results"
  ],
  "author": "tkidman",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.1299.0",
    "axios": "^1.13.4",
    "cheerio": "^1.2.0",
    "copy-dir": "^1.3.0",
    "country-code-lookup": "0.0.16",
    "debug": "^4.3.4",
    "dotenv": "^8.2.0",
    "googleapis": "^51.0.0",
    "handlebars": "^4.7.7",
    "jsdom": "^16.2.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.40",
    "node-html-to-image": "^3.0.0",
    "papaparse": "^5.2.0",
    "puppeteer": "^24.37.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/jest": "^30.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.12.2",
    "eslint-plugin-prettier": "^5.5.5",
    "husky": "^4.2.5",
    "jest": "^30.2.0",
    "prettier": "^3.8.1"
  },
  "husky": {
    "skipCI": false,
    "hooks": {
      "pre-push": "CLUB=test npm run prepush"
    }
  },
  "jest": {
    "testEnvironment": "node"
  }
}
